file = {:home/filiphl/master-thesis/thesis/resources/simpleMicroscopicTheoryOfAmontonsLawsForStaticFriction.pdf:pdf},
issn = {00319007},
journal = {Physical Review Letters},
number = {7},
pages = {1295--1298},
